<div id="focus" class="focus">
	<div class="hd">
		<ul></ul>
	</div>
	<div class="bd">
		<ul>
				<li><img a src="/images/banner.jpg" /></li>
		</ul>
	</div>
</div>


<script type="text/javascript">
    TouchSlide({
        slideCell: "#focus",
        titCell: ".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
        mainCell: ".bd ul",
        effect: "leftLoop",
        autoPlay: false,//自动播放
        autoPage: true, //自动分页
        switchLoad: "_src" //切换加载，真实图片路径为"_src" 
    });

    function changeBranch(obj) {
        var fil = "?";
        if (window.location.href.indexOf("?") != -1)
            fil = "&";
        window.location.href = window.location.href.replace("city_id", "c") + fil + "city_id=" + $(obj).val()+"&entry=";
    }
    
    
</script>